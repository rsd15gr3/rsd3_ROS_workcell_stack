<CollisionSetup>
    <Exclude>
        <!-- Here again we basically have an exclude all type of device. -->

        <FramePair first="Base" second="LeftFinger"/>
        <FramePair first="Base" second="RightFinger"/>

        <!-- We say that the jaws can't collide. -->
        <FramePair first="LeftFinger" second="RightFinger"/>
	<FramePair first="LeftFingerTip" second="RightFingerTip"/>

	<FramePair first="LeftFinger" second="LeftFingerTip" />
	<FramePair first="LeftFinger" second="RightFingerTip" />

	<FramePair first="RightFinger" second="LeftFingerTip" />
	<FramePair first="RightFinger" second="RightFingerTip" />

    </Exclude>
</CollisionSetup>
